{
  "name": "todo",
  "version": "1.0.0",
  "description": "building todo app using mongodb, express, react and nodejs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-watch": "nodemon index.js",
    "dev": "concurrently \"yarn run start-watch\" \"cd client && yarn start\"",
    "code-check": "./node_modules/.bin/eslint '**/*.js' --ignore-pattern node_modules/ client/",
    "build": "webpack --config webpack.config.js"
  },
  "author": "Abdul Rahaman",
  "license": "MIT",
  "dependencies": {
    "atob": "^2.1.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "mongoose": "^5.3.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "quill-emoji": "^0.1.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
